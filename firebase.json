{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/Homepage**",
      "**/Criando a Conta**",
      "**/Onboarding**",
      "**/Sobre NÃ³s**",
      "**/404**",
      "LICENSE**",
      "package-log.json",
      "package.json",
      "**README.md",
      "yarn.lock"
    ],
    "redirects": [{
      "source": "/about-us",
      "destination": "/sobre-nos.html",
      "type": 200
    }, {
      "source": "/sobre-nos",
      "destination": "/sobre-nos.html",
      "type": 200
    }, {
      "source": "/sobre-nosotros",
      "destination": "/sobre-nos.html",
